import { InputfieldComponent } from './inputfield.component';
import { BaseInput } from './base-input';
import { ElementRef, AfterViewInit, EventEmitter } from '@angular/core';
export declare class BmwInputDirective implements AfterViewInit, BaseInput {
    private _el;
    private _inputField;
    private _disabled;
    classes: string[];
    placeholder: string;
    constructor(_el: ElementRef, _inputField: InputfieldComponent);
    classBmwInput: boolean;
    get isDisabled(): string;
    set disabled(value: boolean);
    get disabled(): boolean;
    tabIndex: string;
    protected _id: string;
    protected _type: string;
    set withIcon(value: boolean);
    get id(): string;
    disableToggle: EventEmitter<boolean>;
    set id(value: string);
    get type(): string;
    set type(value: string);
    ngAfterViewInit(): void;
}
