import { ElementRef } from '@angular/core';
import { TooltipPosition, TooltipDisplay } from './tooltip.enum';
import { TooltipPositionService } from './tooltip-position.service';
export declare class TooltipComponent {
    private tooltipPositionService;
    theme: string;
    heading: string;
    content: string;
    position: TooltipPosition;
    display: TooltipDisplay;
    active: boolean;
    disabled: boolean;
    tooltipRef: ElementRef;
    constructor(tooltipPositionService: TooltipPositionService);
    show(): void;
    hide(): void;
    onMouseEnter(): void;
    onMouseLeave(): void;
}
