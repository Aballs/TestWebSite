import { ElementRef, EventEmitter, OnInit, AfterViewInit } from '@angular/core';
import { ContextMenuComponent } from '../context-menu/context-menu.component';
import { CheckboxComponent } from '../checkbox/checkbox.component';
export declare class ListItemComponent implements OnInit, AfterViewInit {
    childContextMenuComponent: ContextMenuComponent;
    contextMenu: ElementRef;
    labelElement: ElementRef;
    checkboxElement: ElementRef;
    id: string;
    selected: boolean;
    icon: string;
    multiSelect: boolean;
    label: string;
    order: string;
    updateEvent: EventEmitter<boolean>;
    withContextMenu: boolean;
    ngOnInit(): void;
    ngAfterViewInit(): void;
    focusOut(e: MouseEvent): void;
    openContextMenu(e: MouseEvent): void;
    toggle(state?: boolean): void;
    checkboxUpdate(event: CheckboxComponent): void;
    click(event: MouseEvent): void;
}
