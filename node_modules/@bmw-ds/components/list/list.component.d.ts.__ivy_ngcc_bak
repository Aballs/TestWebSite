import { AfterViewInit, QueryList, ElementRef, OnDestroy, EventEmitter } from '@angular/core';
import { ListItemComponent } from './list-item.component';
import { CheckboxState } from '../checkbox/checkbox.component';
import { CheckboxComponent } from '../checkbox/checkbox.component';
export declare class ListComponent implements AfterViewInit, OnDestroy {
    input: CheckboxComponent;
    items: ElementRef;
    header: ElementRef;
    headerCheckbox: ElementRef;
    id: string;
    disabled: boolean;
    label: string;
    multiSelect: boolean;
    childItemsComponent: QueryList<ListItemComponent>;
    get selectedItem(): ListItemComponent;
    set selectedItem(value: ListItemComponent);
    private _selectedItem;
    selectedItemChange: EventEmitter<ListItemComponent>;
    selectHeader: CheckboxState;
    sortingOrder: boolean;
    updateEvent: EventEmitter<ListItemComponent | ListItemComponent[]>;
    ngAfterViewInit(): void;
    selectOne(selectItem: ListItemComponent): void;
    selectMore(selectedItem: ListItemComponent): void;
    headerChange(event: CheckboxComponent): void;
    sort(event: MouseEvent): void;
    ngOnDestroy(): void;
}
