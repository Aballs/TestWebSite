import { AfterContentInit, ElementRef, EventEmitter, OnDestroy, QueryList, Renderer2, ChangeDetectorRef } from '@angular/core';
import { BottomNavigationItemComponent } from './bottom-navigation-item.component';
export declare class BottomNavigationComponent implements AfterContentInit, OnDestroy {
    private renderer;
    private cdRef;
    selectionChanged: EventEmitter<{
        item: BottomNavigationItemComponent;
        selected: boolean;
    }>;
    items: QueryList<BottomNavigationItemComponent>;
    menuEntries: QueryList<BottomNavigationItemComponent>;
    moreEntries: Array<BottomNavigationItemComponent>;
    moreButton: BottomNavigationItemComponent;
    moreContainer: ElementRef;
    baseContainer: ElementRef;
    childOfMoreSelected: boolean;
    get moreSelected(): boolean;
    private _moreSelected;
    private onDestroy$;
    id: string;
    onKeydown(event: KeyboardEvent): void;
    constructor(renderer: Renderer2, cdRef: ChangeDetectorRef);
    ngAfterContentInit(): void;
    ngOnDestroy(): void;
    toggleMore(forcedState?: boolean): void;
    private processTransclusion;
    setPreviousItemAsFocused(): void;
    setNextItemAsFocused(): void;
    private selectItem;
}
