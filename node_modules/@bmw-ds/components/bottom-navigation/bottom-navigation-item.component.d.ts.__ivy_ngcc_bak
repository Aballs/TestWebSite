import { ElementRef, OnInit } from '@angular/core';
import { Subject } from 'rxjs';
export declare class BottomNavigationItemComponent implements OnInit {
    private el;
    constructor(el: ElementRef);
    private _selected;
    icon: string;
    label: string;
    set selected(value: boolean);
    get selected(): boolean;
    id: string;
    tabIndex: number;
    selectedChange: Subject<boolean>;
    ngOnInit(): void;
    click(): void;
    get nativeElement(): any;
}
