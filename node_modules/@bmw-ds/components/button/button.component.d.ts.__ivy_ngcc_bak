import { OnInit, EventEmitter, ElementRef, OnChanges } from '@angular/core';
export declare class ButtonComponent implements OnInit, OnChanges {
    elementRef: ElementRef;
    button: string;
    iconRight: string;
    iconLeft: string;
    type: string;
    disabled: boolean;
    id: string;
    tabIndex: number;
    clickEvent: EventEmitter<ButtonComponent>;
    primary: boolean;
    secondary: boolean;
    small: boolean;
    private supportedTypes;
    constructor(elementRef: ElementRef);
    ngOnInit(): void;
    ngOnChanges(): void;
    checkNxProperties(): void;
    click(): void;
}
