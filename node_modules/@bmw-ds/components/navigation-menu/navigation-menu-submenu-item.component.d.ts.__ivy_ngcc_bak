import { NavigationService } from './navigation.service';
import { OnInit, OnDestroy, ElementRef } from '@angular/core';
import { Subject } from 'rxjs';
export declare class NavigationMenuSubmenuItemComponent implements OnInit, OnDestroy {
    private _navService;
    private _selected;
    private _disabled;
    private _destroyed$;
    id: string;
    set disabled(value: boolean);
    get disabled(): boolean;
    set selected(value: boolean);
    get selected(): boolean;
    submenuItemWrapper: ElementRef;
    selectedEvent: Subject<NavigationMenuSubmenuItemComponent>;
    onKeydown(event: KeyboardEvent): void;
    constructor(_navService: NavigationService);
    ngOnInit(): void;
    ngOnDestroy(): void;
    clickEvent($event: any): void;
}
